                    <view key="view" contentMode="scaleToFill" id="cRH-bm-V8d">
                        <rect key="frame" x="0.0" y="0.0" width="375" height="657"/>
                        <autoresizingMask key="autoresizingMask" widthSizable="YES" heightSizable="YES"/>
                        <subviews>
                            <imageView clipsSubviews="YES" userInteractionEnabled="NO" contentMode="scaleAspectFit" horizontalHuggingPriority="251" verticalHuggingPriority="251" image="3.1.png" translatesAutoresizingMaskIntoConstraints="NO" id="x8a-nc-ppg">
                                <rect key="frame" x="0.0" y="0.0" width="375" height="375"/>
                                <constraints>
                                    <constraint firstAttribute="width" secondItem="x8a-nc-ppg" secondAttribute="height" multiplier="1:1" id="0tI-ZD-qfw"/>
                                </constraints>
