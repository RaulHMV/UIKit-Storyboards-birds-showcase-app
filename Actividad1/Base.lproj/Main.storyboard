                            <imageView clipsSubviews="YES" userInteractionEnabled="NO" contentMode="scaleAspectFit" horizontalHuggingPriority="251" verticalHuggingPriority="251" image="4.1.png" translatesAutoresizingMaskIntoConstraints="NO" id="YYT-27-Nxs">
